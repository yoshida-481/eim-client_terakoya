    "@angular/forms": "^19.2.8",
    "@angular/localize": "^19.2.8",
    "@angular/material": "^19.2.11",
    "@angular/platform-browser": "^19.2.8",
    "@angular/platform-browser-dynamic": "^19.2.8",
    "@angular/platform-server": "^19.2.8",
    "@angular/router": "^19.2.8",
    "@angular/upgrade": "^19.2.8",
    "@date-fns/tz": "^1.1.2",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "@primeng/themes": "^19.1.2",
    "@worktile/gantt": "^19.0.7",
    "ag-grid-angular": "33.0.4",
    "ag-grid-community": "33.0.4",
    "angular-in-memory-web-api": "^0.19.0",
    "angular-split": "^19.0.0",
