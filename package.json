{
  "name": "eim-client",
  "version": "6.14.0",
  "license": "",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=3096 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.js --live-reload false",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "docs": "typedoc --out ./docs/ ./src/ --experimentalDecorators --mode file --ignoreCompilerErrors --hideGenerator --excludePrivate",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json -d docs --disableSourceCode --disableCoverage --customFavicon ./src/favicon.ico --disablePrivate",
    "yuidocs": "cpx ../release_work/webapp/app/document/html/main.bundle.js work && yuidoc 2> yuidoc-error.log",
    "i18n": "ng-xi18n",
    "build": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --configuration production --aot && cd release/webapp/client && bestzip ../client.zip *",
    "copyrights": "license-checker --customPath copyrightsFormat.json --production --csv --out copyrights.csv",
    "build-eimform-client-template": "node create-eimform-client-template.js",
    "build-eim-task-client-template": "node create-eim-task-client-template.js"
  },
  "private": true,
  "dependencies": {
