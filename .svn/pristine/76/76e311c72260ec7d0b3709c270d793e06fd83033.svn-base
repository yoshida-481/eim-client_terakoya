<div style="height: 100%; padding-top: 0px; padding-left: 0px; padding-bottom: 0px;">
	<div class="eim-form-item">
		<label style="padding-right: 5px;"><i class="fa eim-icon-info" style="color: #0d98ed;"></i></label>
		<div style="width: 100%;">
			<label class="eim-form-label" style="width: 25%;">{{confirmMessage}}</label>
		</div>
	</div>
	<div class="eim-form-item">
		<div class="eim-form-input eim-form-input-selected-list" style="display: flex; width: 100%;">
			<div style="width: 25%; align-self: center;">
				<label class="eim-form-label" style="width: 25%;">{{ 'EIM_OBJECT_EDITORS.LABEL_02022' | translate }}</label>
			</div>
			<input type="text" name="lockUserName" pInputText [ngModel]="user.name" [disabled]="true" style="width: calc(75% - 28px);"/>
			<button pButton type="button" icon="fa fa-fw fa fa-external-link" style="font-size: 12px; margin-left: 5px;" severity="info" class="icon-only-button" pTooltip="{{ 'EIM.LABEL_03006' | translate }}"(click)="onClickUserSelector()"></button>
		</div>
	</div>
</div>