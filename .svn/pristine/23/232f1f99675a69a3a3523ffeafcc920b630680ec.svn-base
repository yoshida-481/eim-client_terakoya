<form #pdfSettingForm="ngForm" class="eim-form-wide" style="height: 100%; margin-right: -2px;" autocomplete="off"
	novalidate>
	<p-tabs [value]="0">
		<p-tablist>
			<p-tab [value]="0">{{ 'EIM_DOCUMENTS.LABEL_02099' | translate }}</p-tab>
			<p-tab [value]="1">{{ 'EIM_DOCUMENTS.LABEL_02003' | translate }}</p-tab>
		</p-tablist>
		<p-tabpanels>
			<p-tabpanel [value]="0">
				<div style="padding:5px; height: 100%">
					<eim-pdf-electronic-signature-applicable #pdfElectronicSignatureApplicable
						[activityFlag]="activityFlag" [objId]="objId">
					</eim-pdf-electronic-signature-applicable>
				</div>
			</p-tabpanel>
			<p-tabpanel [value]="1">
				<div style="padding:5px; height: 100%">
					<div style="height: 100%" eim-vertical-container>
						<div style="width: 100%; margin-bottom: 3px;">
							<as-split direction="horizontal" style="display: flex;">
								<eim-radio-button class="eim-form-item" name="doSetSecurity"
									label="{{ 'EIM.LABEL_02044' | translate }}" value="true" [(ngModel)]="doSetSecurity"
									[disabled]="securityDisabled != false"
									(click)="onClickBlockOn($event)"></eim-radio-button>
								<div style="padding-left: 20px;"></div>
								<eim-radio-button class="eim-form-item" name="doSetSecurity"
									label="{{ 'EIM.LABEL_02045' | translate }}" value="false"
									[(ngModel)]="doSetSecurity" [disabled]="securityDisabled != false"
									(click)="onClickBlockOn($event)"></eim-radio-button>
							</as-split>
						</div>
						<div class="eim-flexible-height">
							<eim-public-file-security-executor #publicFileSecurityUpdater [activityFlag]="activityFlag"
								[objId]="objId" [publicFileSecurity]="pdfOutput">
							</eim-public-file-security-executor>
						</div>
					</div>
				</div>
			</p-tabpanel>
		</p-tabpanels>
	</p-tabs>

</form>