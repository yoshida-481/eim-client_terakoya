.eim-version-display {
	width: 100%;
	height: 100%;
	/* 上下左右中央寄せ指定 */
	position: relative;
}

.eim-version-display-detail {
	/* 上下左右中央寄せ指定 */
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	margin: auto;
	width: 250px;
	height: 80px;
}

