<div style="width: 100%; height: 155px;" class="eim-contents-search">
	<p-panel class="eim-panel-containing-grid" header="{{'EIM_ADMINS.LABEL_02118' | translate}}">
		<form #conditionForm="ngForm" eim-vertical-container style="width: 100%; background-color: #FFFFFF;" autocomplete="off" novalidate>
			<div class="p-grid basic-condition" style="width: 100%;">
				<div class="p-col-6" style="padding-left: 0px; padding-right: 0px; width: 900px;">
					<div class="eim-form-item">
						<label class="eim-form-label" style="width: 100px;">{{ 'EIM_ADMINS.LABEL_02120' | translate }}</label>
						<div class="eim-form-input" style="width: 70%;">
							<eim-radio-button name="searchType" label="{{'EIM_ADMINS.LABEL_02121' | translate}}" value="allPeriodss"
								[(ngModel)]="searchType" (ngModelChange)="onChangeCheckSearch()"></eim-radio-button>
							<eim-radio-button name="searchType" label="{{'EIM_ADMINS.LABEL_02122' | translate}}" value="specifyPeriod"
								[(ngModel)]="searchType" (ngModelChange)="onChangeCheckSearch()"></eim-radio-button>
						</div>
					</div>
					<div class="eim-form-item" >
						<label class="eim-form-label" style="width: 100px;"></label>
						<div class="eim-form-input" style="display: flex; width: 750px; height: 100%;">
							<!-- 期間を指定（From） -->
							<div style="width: 257px; display: flex;">
								<div style="width: 130px;">
									<p-datepicker #fromCalendar [disabled]="searchType != 'specifyPeriod'" name="fieldFrom" [(ngModel)]="inputValueFrom" [selectOtherMonths]="true"
										dateFormat="{{dateFormat}}" [showIcon]="true" appendTo="body" [showButtonBar]="true" [monthNavigator]="true" [yearNavigator]="true"  [showOnFocus]=false
										[yearRange]="CALENDAR_YEAR_RANGE" [inputStyle]="{'width':'70%'}" (ngModelChange)="onChangeCheckSearch()"
										(click)="openCalendar(fromCalendar)" (onTodayClick)="onClickToday($event, fromCalendar)" (onClearClick)="onClickClear($event, fromCalendar)"></p-datepicker>
								</div>
								<div style="width: 55px; margin-left: 10px;">
									<p-select [disabled]="searchType != 'specifyPeriod' || !inputValueFrom" name="fromHH" [options]="listHH" [(ngModel)]="inputHhFrom"
										placeholder="{{'EIM.INFO_00002' | translate}}" appendTo="body" [style]="{'width': '50px'}" (ngModelChange)="onChangeCheckSearch()"></p-select>
								</div>
								<label class="eim-form-label" style="width: 10px; display: flex; align-items: center; justify-content: center;">{{DELIMITER_COLON}}</label>
								<div style="width: 55px;">
									<p-select [disabled]="searchType != 'specifyPeriod' || !inputValueFrom" name="fromMM" [options]="listMM" [(ngModel)]="inputMmFrom"
										placeholder="{{'EIM.INFO_00002' | translate}}" appendTo="body" [style]="{'width': '50px'}" (ngModelChange)="onChangeCheckSearch()"></p-select>
								</div>
							</div>
							<label class="eim-form-label" style="width: 20px; display: flex; align-items: center; justify-content: center;">{{DELIMITER_TIDE}}</label>
							<!-- 期間を指定（To） -->
							<div style="width: 257px; display: flex;">
								<div style="width: 130px;">
									<p-datepicker #toCalendar [disabled]="searchType != 'specifyPeriod'" name="fieldTo" [(ngModel)]="inputValueTo" [selectOtherMonths]="true" dateFormat="{{dateFormat}}"
										[showIcon]="true" appendTo="body" [showButtonBar]="true" [monthNavigator]="true" [yearNavigator]="true" [yearRange]="CALENDAR_YEAR_RANGE"  [showOnFocus]=false
										[inputStyle]="{'width':'70%'}" (ngModelChange)="onChangeCheckSearch()" (click)="openCalendar(toCalendar)" (onTodayClick)="onClickToday($event, toCalendar)" (onClearClick)="onClickClear($event, toCalendar)"></p-datepicker>
								</div>
								<div style="width: 55px; margin-left: 10px;">
									<p-select [disabled]="searchType != 'specifyPeriod' || !inputValueTo" name="toHH" [options]="listHH" [(ngModel)]="inputHhTo"
										placeholder="{{'EIM.INFO_00002' | translate}}" appendTo="body" [style]="{'width': '50px'}" (ngModelChange)="onChangeCheckSearch()"></p-select>
								</div>
								<label class="eim-form-label" style="width: 10px; display: flex; align-items: center; justify-content: center;">{{DELIMITER_COLON}}</label>
								<div style="width: 55px;">
									<p-select [disabled]="searchType != 'specifyPeriod' || !inputValueTo" name="toMM" [options]="listMM" [(ngModel)]="inputMmTo"
										placeholder="{{'EIM.INFO_00002' | translate}}" appendTo="body" [style]="{'width': '50px'}" (ngModelChange)="onChangeCheckSearch()"></p-select>
								</div>
							</div>
						</div>
					</div>

					<!-- ユーザ選択方法 -->
					<div class="eim-form-item">
						<label class="eim-form-label" style="width: 100px;">{{ 'EIM_ADMINS.LABEL_02123' | translate }}</label>
						<div class="eim-form-input" style="width: 70%;">
							<eim-radio-button name="allUser" label="{{'EIM_ADMINS.LABEL_02124' | translate}}" value="allUsers" [(ngModel)]="allUser" (ngModelChange)="onChangeCheckSearch()"></eim-radio-button>
							<eim-radio-button name="allUser" label="{{'EIM_ADMINS.LABEL_02125' | translate}}" value="specifyUser" [(ngModel)]="allUser" (ngModelChange)="onChangeCheckSearch()"></eim-radio-button>
						</div>
					</div>
				</div>
			</div>
			<!-- ユーザを指定 -->
			<div class="p-grid basic-condition" style="width: 100%; display: flex;">
				<div class="p-col-6" style="padding-left: 0px; padding-right: 0px; width: 360px;">
					<div class="eim-form-item">
						<label class="eim-form-label" style="width: 100px;"></label>
						<div class="eim-form-input" style="display: flex; margin-left: 10px;">
							<input [ngClass]="allUser == 'specifyUser' ? 'search-target-user' : ''" [disabled]="allUser != 'specifyUser'" name="userId" type="text" pInputText
								[(ngModel)]="userName" style="width:200px; margin-right: 5px;" readonly="readonly"/>
							<button pButton type="button" icon="fa-fw fa fa-external-link" severity="info" class="icon-only-button" style="float: right;" pTooltip="{{ 'EIM.LABEL_03006' | translate }}"
								(click)="onShowUserSelector($event);" [disabled]="allUser != 'specifyUser'"></button>
						</div>
					</div>
				</div>
				<!-- 検索ボタン -->
				<div class="p-col-6" style="padding-left: 5px; width: calc(100% - 360px); display: flex;">
					<div style="margin-left: 5px; margin-top: 3px;">
						<button pButton label="{{'EIM_ADMINS.LABEL_03024' | translate}}" icon="eim-icon-search"
							class="eim-contents-search-btn p-button-primary"
							(click)="onSearch($event, conditionForm)"[disabled]="(disabledCheckSearchButton === false) ? false : true"></button>
					</div>
					<div style="margin-left: 10px; margin-top: 3px;">
						<button pButton label="{{'EIM_ADMINS.LABEL_03025' | translate}}" icon="fa fa-download"
							class="eim-contents-search-btn p-button-primary"
							(click)="onClickCsvDownload($event)" [disabled]="disabledCheckDownloadCSV()"></button>
					</div>
					<div style="margin-left: 10px; margin-top: 3px;">
						<button pButton label="{{'EIM_ADMINS.LABEL_03026' | translate}}" icon="fa fa-download"
							class="eim-contents-search-btn p-button-primary"
							(click)="onClickSearchAndCsvDownload($event)"[disabled]="disabledCheckSearchButton"></button>
					</div>
				</div>
			</div>
		</form>
	</p-panel>
</div>
<div style="height: calc(100% - 155px); ">
	<p-panel class="eim-panel-containing-grid" header="{{'EIM_ADMINS.LABEL_02130' | translate}}">
		<div style="height: 100%;" eim-vertical-container>
			<div class="eim-flexible-height">
				<eim-data-grid #opeHistoryListGrid [equals]="opeHistoryListGridEquals" [multiple]="false"></eim-data-grid>
			</div>
		</div>
	</p-panel>
</div>
