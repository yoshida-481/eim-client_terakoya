#cy {
	width: 100%;
	height: 100%;
	display: block;
	position: absolute;
	left: 0px;
	top: 0px;
}

.eim-ui-widget {
	position: relative;
	width: 100%;
	height: 100%;
	border-style: solid;
	border-width: 1px;
	border-color: #d6d6d6;
	background: d6d6d6;
}

.eim-diagram-title {
	position: absolute;
	margin: 6px;
}

.eim-diagram-action {
	padding-top: 6px;
	padding-right: 6px;
	float: right;
}

.eim-diagram-action > button {
	margin-left: 5px;
	margin-right: 0px;
}

.eim-diagram-action-center {
	padding-top: 6px;
	position: absolute;
	margin-left: 300px;
	display: -webkit-inline-box;
	display : -webkit-inline-box;
	display : -ms-inline-flexbox;
	display : -webkit-inline-flex;
	display : inline-flex;
}

.p-togglebutton {
	display: inline;
	border: 0px!important;
    padding: 0px;
	font-size: 12px;
}

::ng-deep .p-togglebutton-content {
	padding: 0.429rem 0!important;
    background: linear-gradient(to bottom, #30B759, #35A86A)!important;
    border-radius: 3px!important;
	text-align: center;
	width: 25px;
    height: 25px;
}
::ng-deep .p-togglebutton.p-togglebutton-checked > .p-togglebutton-content:not(:hover) {
	background: linear-gradient(to bottom, rgb(32, 100, 52), rgb(12, 70, 39))!important;
}
::ng-deep .p-togglebutton:not(.p-togglebutton-checked) > .p-togglebutton-content:hover {
	background: #c8c8c8!important;
}
::ng-deep .p-togglebutton.p-togglebutton-checked > .p-togglebutton-content:hover {
	background: #116fbf!important;
}

::ng-deep .p-togglebutton-icon {
	width: 1em;
    margin-left: -0.5em;
    color: #FFFFFF!important;
	position: absolute!important;
    left: 50%;
}
::ng-deep .p-togglebutton:not(.p-togglebutton-checked) > .p-togglebutton-content:hover > .p-togglebutton-icon {
	color: #212121!important;
}
