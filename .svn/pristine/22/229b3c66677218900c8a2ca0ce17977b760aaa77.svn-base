<div eim-vertical-container style="height: 100%; padding-top: 0px; padding-left: 0px; padding-bottom: 0px;">
	<div class="eim-form-item" style="width: 100%;">
		<label class="eim-form-label" style="margin-top: 3px; width: 25%;">{{ 'EIM_OBJECT_EDITORS.LABEL_02015' | translate }} </label>
		<div class="eim-form-input eim-form-input-selected-list" style="width: 75%; float:right;">
			<input type="text" name="workflowLabel" pInputText [ngModel]="workflow.name"  style="width: 100%" [disabled]="true" />
		</div>
	</div>
	<div class="eim-form-item" style="width: 100%;">
		<label class="eim-form-label" style="width: 25%;">{{ 'EIM_OBJECT_EDITORS.LABEL_02023' | translate }} </label>
		<div class="eim-form-input eim-form-input-selected-list" style="width: 75%; float:right;">
			<input type="text" name="statusLabel" pInputText [ngModel]="nowStatus.typeName" style="width: 100%" [disabled]="true"/>
		</div>
	</div>
	<div class="eim-data-grid-menu">
		<eim-menubar [model]="assignMenuItems"></eim-menubar>
	</div>

	<div class="eim-flexible-height" style="width: 100%;">
		<eim-data-grid #assignDataGrid 
				[equals]="equals"
				[contextMenuItems]="assignContextMenuItems" 
				(selected)="onSelectAssignDataGrid()">
		</eim-data-grid>
	</div>

</div>
