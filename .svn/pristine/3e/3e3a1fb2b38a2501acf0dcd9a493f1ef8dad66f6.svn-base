#cy {
  width: 100%;
  height: 100%;
  display: block;
  position: absolute;
  left: 0px;
  top: 0px;
}

.eim-ui-widget {
	position: relative;
  width: 100%;
  height: 100%;
	border-style: solid;
	border-width: 1px;
  border-color: #d6d6d6;
}

.eim-diagram-title {
  position: absolute;
  margin: 6px;
}

.eim-diagram-action {
  padding-top: 6px;
  padding-right: 6px;
  float: right;
}

.eim-diagram-action > button {
	margin-left: 5px;
	margin-right: 0px;
}
