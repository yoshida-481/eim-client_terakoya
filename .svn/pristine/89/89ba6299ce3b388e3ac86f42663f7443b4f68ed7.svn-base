<div class="p-grid" style="height: calc(100% - 30px); width: 100%;">

	<div class="eim-form-csv-download-panel" style="height: 100%; width: calc(50% - 40px); padding-right: 6px;">
		<p-panel class="eim-panel-containing-grid" header="{{ 'EIM_FORMS.LABEL_02039' | translate }}">
			<form style="height:40px; display: flex;" (ngSubmit)="onClickFilter($event)" autocomplete="off">
				<div class="eim-form-item" style="width: 100%;">
					<div class="eim-form-input" style="width: 100%; display: block !important;">
						<input pInputText style="width:100%;" type="text" name="filterStr" style="width: 100%;" [maxlength]="maxlength" [(ngModel)]="filterCondition.filterStr" />
					</div>
				</div>
				<div class="p-rtl" dir="rtl" style="padding-top: 3px; padding-left: 3px;">
					<button pButton class="p-button-primary" icon="eim-icon-filter" style="margin-right: 0px; height: 26px; width: 26px;" pTooltip="{{ 'EIM.LABEL_03031' | translate }}" (click)="onClickFilter($event)"></button>
				</div>
			</form>
			<div style="height: calc(100% - 40px);">
				<eim-data-grid #attributeTypeListGrid></eim-data-grid>
			</div>
		</p-panel>
	</div>

	<div class="eim-form-csv-download-middle-panel" style="height: 100%; width: 40px;">
		<p-panel class="eim-panel-containing-grid" header="ã€€">
			<div style="display: flex; height: 100%;">
				<div style="display: table; height: 100%;">
					<div style="vertical-align: middle; float: none; display: table-cell;">
						<button pButton class="p-button-success eim_form_csv_move_button" type="button" icon="fa fa-arrow-right"
								style="margin-right: 5px; margin-bottom: 10px;"
								pTooltip="{{ 'EIM.LABEL_03010' | translate }}" (click)="onClickMoveRight($event)">
						</button>
						<button pButton class="p-button-success eim_form_csv_move_button" type="button" icon="fa fa-arrow-left"
								style="margin-right: 5px;"
								pTooltip="{{ 'EIM.LABEL_03003' | translate }}" (click)="onClickMoveLeft($event)">
						</button>
					</div>
				</div>
			</div>
		</p-panel>
	</div>

	<div class="eim-form-csv-download-panel" style="height:100%; width:50%; padding-left:6px;">
		<p-panel class="eim-panel-containing-grid" header="{{ 'EIM_FORMS.LABEL_02040' | translate }}">

			<p-header>
				<span class="eim-icon-info" style="padding-left: 3px; color: #0d98ed;"
						pTooltip="{{ 'EIM_FORMS.LABEL_02041' | translate }}">
				</span>
			</p-header>

			<div style="height: 100%; width: 100%; display: flex;">
				<div style="width: calc(100% - 35px);">
					<eim-data-grid #downloadAttributeTypeListGrid></eim-data-grid>
				</div>
				<div style="height: 100%; width: 35px; display: table;">
					<div style="vertical-align:middle; float:none; display:table-cell;">
						<button pButton class="p-button-success eim_form_csv_move_button" type="button" icon="fa fa-arrow-up"
								style="margin-bottom: 10px;"
								pTooltip="{{ 'EIM.LABEL_03029' | translate }}" (click)="onClickMoveUp($event)"></button>
						<button pButton class="p-button-success eim_form_csv_move_button" type="button" icon="fa fa-arrow-down"
								pTooltip="{{ 'EIM.LABEL_03030' | translate }}" (click)="onClickMoveDown($event)"></button>
					</div>
				</div>
			</div>
		</p-panel>
	</div>

</div>