div.container {
  overflow: hidden;
}
div.clear {
  clear: both;
}

.eim-object-editor-main {
	width: 100%;
	height: 100%;
}

::ng-deep .eim-object-editor-main .mat-mdc-tab-group {
	font-family: "メイリオ", "Meiryo UI", "sans-serif";
	color: #555;
}

::ng-deep .eim-object-editor-main .mat-mdc-tab {
	font-family: "メイリオ", "Meiryo UI", "sans-serif";
	color: #555;
	opacity: 1;
	height: 30px;
	padding: 0px 14px 0px 14px;
	margin-right: 2px;
	min-width: inherit;
	background: #ededf0;
	border-color: silver;
	border-top-left-radius: 3px;
	border-top-right-radius: 3px;
	border: 1px solid #d6d6d6;
	border-bottom: 0;
}

::ng-deep .eim-object-editor-main .mat-mdc-tab-labels {
	margin-left: 6px;
	margin-right: 6px;
	--mat-tab-header-label-text-size: 12px;
}

::ng-deep .eim-object-editor-main .mdc-tab--active {
	background-color: #007ad9;
	border-color: #007ad9;
    color: #fff;
	--mdc-text-button-label-text-color: #fff;
	--mat-tab-header-active-focus-label-text-color: #fff;
}

::ng-deep .eim-object-editor-main .mat-mdc-tab-body-wrapper {
	height: 100%;
	width: 100%;
	padding: 3px;
}

::ng-deep .eim-object-editor-main .mat-ink-bar {
	height: 0;
}

::ng-deep .eim-object-editor-main .mat-mdc-button {
	padding: 0px;
	min-width: inherit;
}

.eim-object-editor-main em {
	font-style: normal;
}

/* ==========================================================================
   Angular Material
   ========================================================================== */
::ng-deep .eim-object-editor-main .mat-tab-body-content {
	overflow: hidden;
}

