<div class="eim-form-item">
	<label *ngIf="label" [ngClass]="{'eim-form-label': true, 'eim-form-label-required': required}">{{label}}</label>

	<div class="eim-form-input eim-form-input-selected-list" style="width: 100%; display: flex;">
		<div style="height: 70px; width: 100%;">
			<p-autoComplete [multiple]="true"  name="{{name}}" [ngClass]="{
			        'ng-invalid': (formControls[0] ? (formControls[0].dirty && formControls[0].invalid) : false),
			        'ng-dirty': (formControls[0] ? (formControls[0].dirty) : false),
			        'eim-workspace-administrator-item-list eim-form-input-multi-input-area': !disabled
			    }" [ngModel]="users" [disabled]="true" [suggestions]="[]">
				<ng-template let-user pTemplate="selectedItem">
					<i class="{{user.icon}}" style="padding-right: 3px;"></i>
					<span>{{user.label}}</span>
					<span *ngIf="!disabled && !readOnly">
						<a href="#" style="padding-left:3px; padding-right:1px; color: #E3F1FF"
								pTooltip="{{ 'EIM.LABEL_03003' | translate }}"
								(click)="onClickDelete($event, user.value)"><i class="fa fa-times fa-fw eim-workspace-administrator-item-delete"></i></a>
					</span>
				</ng-template>
			</p-autoComplete>
			</div>
			<div>
				<button pButton *ngIf="!readOnly" type="button" severity="info" class="icon-only-button" icon="fa fa-external-link"
						(click)="onClickSearch($event)" style="margin-left: 5px; float: right;"pTooltip="{{ 'EIM.LABEL_03006' | translate }}"></button>
			</div>
	</div>
</div>