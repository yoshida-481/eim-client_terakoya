<eim-template-data-grid-single-selector #template
	[componentService]="formatSelectorComponentService" [(condition)]="formatSearchCondition"
	[multiple]="multiple">
	<div class="eim-form-item" style="width: 100%;">
		<label class="eim-form-label" style="width: 60px;">{{ 'EIM_ADMINS.LABEL_02016' | translate }}</label>
		<div class="eim-form-input" style="width: calc(100% - 60px); display: block !important;">
			<input pInputText type="text" style="width: 100%;" name="formatName" maxlength="{{inputMaxLength}}"
				[(ngModel)]="formatSearchCondition.formatName" />
		</div>
	</div>
</eim-template-data-grid-single-selector>
