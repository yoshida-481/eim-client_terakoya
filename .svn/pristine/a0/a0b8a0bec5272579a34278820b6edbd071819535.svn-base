::ng-deep .eim-time-calendar-input-form-item {
	padding-left: 6px;
}
::ng-deep .eim-time-calendar-input-form-item .p-inputnumber-input {
	width: 50px;
	padding-right: 1.75em;
	text-align: right;
}
::ng-deep .eim-time-calendar-input-form-item .p-inputnumber-button {
	width:1.5em;
}
::ng-deep .eim-time-calendar-input-form-item .p-inputnumber .p-inputnumber-button .pi {
	font-size:12px;
}

::ng-deep eim-flat-type-attribute-input-form-item.p-col-6 eim-calendar-input-form-item div.eim-form-item p-datepicker .p-datepicker .p-inputtext{
	max-width: 124px;
	width: 100%;
}

::ng-deep .eim-form-wide .eim-form-input-surround {
	border: transparent 0px solid;
	padding: 0px;
	display: flex;
}