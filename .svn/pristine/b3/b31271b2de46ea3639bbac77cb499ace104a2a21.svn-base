<div style="height: 100%; overflow-y: auto; overflow-x: hidden;">
	<form #ngForm="ngForm" class="eim-form-wide" style="height: 100%; overflow-y: auto;" autocomplete="off" novalidate>
		<eim-task-form #propertyForm [form]="ngForm.form" (changed)="onChangedStaticInputFormItemValue($event)">
		</eim-task-form>
		<eim-task-form #processForm [form]="ngForm.form" (changed)="onChangedCustomAttributeValue($event)">
		</eim-task-form>
	</form>
</div>
