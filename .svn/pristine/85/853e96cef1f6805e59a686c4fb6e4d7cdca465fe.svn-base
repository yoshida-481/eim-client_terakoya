<div class="p-grid eim-entry-selector" style="width: 100%; height: 100%;">

	<div style="width: calc(50% - 20px); height: 100%;">
		<p-tabs #entryTabView [value]="selectedTab()" (valueChange)="onChangeTab($event)">
			<p-tablist>
				<p-tab [value]="0">{{ 'EIM.LABEL_02017' | translate }}</p-tab>
				<p-tab [value]="1" *ngIf="selectTarget.group">{{ 'EIM.LABEL_02003' | translate }}</p-tab>
				<p-tab [value]="2" *ngIf="selectTarget.role">{{ 'EIM.LABEL_02004' | translate }}</p-tab>
				<p-tab [value]="3" *ngIf="selectTarget.compGroup">{{ 'EIM.LABEL_02018' | translate }}</p-tab>
				<p-tab [value]="4" *ngIf="selectTarget.userDefGroup">{{ 'EIM.LABEL_02043' | translate }}</p-tab>
				<p-tab [value]="5" *ngIf="selectTarget.system">{{ 'EIM.LABEL_02055' | translate }}</p-tab>
				<p-tab [value]="6" *ngIf="selectTarget.template">{{ 'EIM.LABEL_03057' | translate }}</p-tab>
				<p-tab [value]="7" *ngIf="selectTarget.objectRole">{{ 'EIM.LABEL_02054' | translate }}</p-tab>
			</p-tablist>
			<p-tabpanels>
				<p-tabpanel [value]="0">
					<div style="padding:5px; height: 100%; height: 100%">
						<form style="height: 100%; margin-top: -2px;" (ngSubmit)="onClickUserSearh($event)" autocomplete="off">
							<div eim-vertical-container adjustedValue="-2" style="height: 100%;">
								<div class="eim-form-item">
									<div style="width: 100%; display: flex;" class="eim-form-input">
										<input type="text" class="p-inputtext" pInputText name="keyword" style="width: 100%;"
											[(ngModel)]="keyword" [maxlength]="inputMaxLength" />
										<div style="padding-left: 3px;">
											<button pButton type="button" severity="info" class="icon-only-button"
												icon="fa-fw eim-icon-search" pTooltip="{{ 'EIM.LABEL_03007' | translate }}"
												(click)="onClickUserSearh($event)"></button>
										</div>
									</div>
								</div>
								<div class="eim-flexible-height">
									<eim-data-grid class="eim-user-list" #userListGrid [equals]="defaultEquals"></eim-data-grid>
								</div>
							</div>
						</form>
					</div>
				</p-tabpanel>
				<p-tabpanel [value]="1">
					<div style="padding:5px; width: 100%; height: 100%">
						<div style="height: 50%">
							<eim-tree #groupTree [componentService]="treeComponentService"
								(selected)="onSelectGruopTree($event)"></eim-tree>
						</div>
						<div style="height: 50%">
							<eim-data-grid #groupUserListGrid [equals]="defaultEquals"
								(selected)="onSelectGruopUserList($event)"></eim-data-grid>
						</div>
					</div>
				</p-tabpanel>
				<p-tabpanel [value]="2">
					<div style="padding:5px; width: 100%; height: 100%">
						<div style="height: 50%">
							<eim-tree #roleTree [componentService]="treeComponentService"
								(selected)="onSelectRoleTree($event)"></eim-tree>
						</div>
						<div style="height: 50%">
							<eim-data-grid #roleUserListGrid [equals]="defaultEquals"
								(selected)="onSelectRoleUserList($event)"></eim-data-grid>
						</div>
					</div>
				</p-tabpanel>
				<p-tabpanel [value]="3">
					<div style="padding:5px; width: 100%; height: 100%">
						<div style="height: 50%">
							<eim-tree #complexGroupTree [componentService]="treeComponentService"
								(selected)="onSelectComplexGroupTree($event)"></eim-tree>
						</div>
						<div style="height: 50%">
							<eim-data-grid #complexGroupUserListGrid [equals]="defaultEquals"
								(selected)="onSelectComplexGroupUserList($event)"></eim-data-grid>
						</div>
					</div>
				</p-tabpanel>
				<p-tabpanel [value]="4">
					<div style="padding:5px; width: 100%; height: 100%">
						<div style="height: 100%">
							<eim-data-grid class="eim-user-list" #userDefGroupListGrid [equals]="defaultEquals"></eim-data-grid>
						</div>
					</div>
				</p-tabpanel>
				<p-tabpanel [value]="5">
					<div style="padding:5px; width: 100%; height: 100%">
						<div style="height: 100%">
							<eim-data-grid class="eim-user-list" #systemFuncListGrid [equals]="defaultEquals"></eim-data-grid>
						</div>
					</div>
				</p-tabpanel>
				<p-tabpanel [value]="6">
					<div style="padding:5px; width: 100%; height: 100%">
						<div eim-vertical-container style="height: 100%;">
							<div class="eim-data-grid-menu">
								<eim-menubar [model]="templateMenuItems"></eim-menubar>
							</div>
							<div class="eim-flexible-height">
								<div style="height: 50%">
									<eim-tree #templateTree [componentService]="treeComponentService"
										(selected)="onSelectTemplateTree($event)"></eim-tree>
								</div>
								<div style="height: 50%">
									<eim-data-grid #templateAssignListGrid [equals]="defaultEquals"
										(selected)="onSelectTemplateAssignList($event)"></eim-data-grid>
								</div>
							</div>
						</div>
					</div>
				</p-tabpanel>
				<p-tabpanel [value]="7">
					<div style="padding:5px; width: 100%; height: 100%">
						<div style="height: 100%">
							<eim-data-grid class="eim-user-list" #objectRoleListGrid [equals]="defaultEquals"></eim-data-grid>
						</div>
					</div>
				</p-tabpanel>
			</p-tabpanels>
		</p-tabs>
	</div>

	<div style="height: 100%; width: 40px; display: table;">
		<div style="vertical-align: middle; float: none; display: table-cell;">
			<button pButton class="p-button-success" type="button" icon="fa fa-arrow-right" pTooltip="{{ 'EIM.LABEL_03010' | translate }}"
					appendTo="body" positionStyle="fixed" style="width: 30px; height: 30px; margin-left: 5px; margin-right: 5px; margin-bottom: 10px;"
					(click)="onSelectEntryAdd($event)">
			</button>
			<button pButton class="p-button-success" type="button" icon="fa fa-arrow-left" pTooltip="{{ 'EIM.LABEL_03003' | translate }}"
					appendTo="body" positionStyle="fixed" style="width: 30px; height: 30px; margin-left: 5px; margin-right: 5px;"
					(click)="onSelectEntryDelete($event)"></button>
		</div>
	</div>

	<div eim-vertical-container style="height: 100%; width: calc(50% - 20px);">
		<div class="eim-flexible-height" style="height: 100%;">

			<div class="eim-entry-selector-destination">
				<p-panel header="{{ 'EIM.LABEL_02038' | translate }}" class="eim-panel-containing-grid">
					<eim-data-grid #destinationListGrid [equals] = "defaultEquals"></eim-data-grid>
				</p-panel>
			</div>

		</div>
	</div>

</div>
