::ng-deep .eim-form-csv-download-panel .p-panel {
	height: 100%;
}

::ng-deep .eim-form-csv-download-panel .p-panel-content-container.ng-trigger-panelContent {
	height: calc(100% - 20px);
	padding-bottom: 5px;
}

::ng-deep .eim-form-csv-download-panel .p-panel-content {
	height: 100%;
}
/* 2022.05 新規 iアイコンのずれ対応 */
::ng-deep .eim-form-csv-download-panel .p-panel-header {
	display: block;
}

::ng-deep .eim-form-csv-download-middle-panel .p-panel {
	height: 100%;
}

.eim_form_csv_move_button {
	width: 30px;
	height: 30px;
	margin-left: 5px;
}

/* ==========================================================================
   左グリッドと右グリッドの矢印ボタンの垂直中央位置を合わせるためのパネル
   ========================================================================== */
::ng-deep .eim-form-csv-download-middle-panel .p-panel-content-container.ng-trigger-panelContent {
	height: calc(100% - 20px);
	padding-bottom: 5px;
}

::ng-deep .eim-form-csv-download-middle-panel .p-panel-content {
	height: 100%;
}

::ng-deep .eim-form-csv-download-middle-panel .p-panel-header {
	border-bottom-color: #FFFFFF !important;
}

::ng-deep .eim-form-csv-download-middle-panel .p-panel-header span {
	visibility: hidden;
}